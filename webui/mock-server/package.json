{
  "name": "ah-webui-mock-server",
  "version": "1.0.0",
  "description": "Mock REST API server for the Agent Harbor WebUI development and testing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "format:check": "prettier --check src/**/*.{ts,js,json}",
    "type-check": "tsc --noEmit",
    "test": "echo \"Tests handled by e2e-tests package\" && exit 0"
  },
  "keywords": [
    "mock",
    "api",
    "rest",
    "agent-harbor"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.18.6",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=22"
  },
  "types": "./dist/index.d.ts"
}
