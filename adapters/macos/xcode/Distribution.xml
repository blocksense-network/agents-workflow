<?xml version="1.0" encoding="utf-8"?>
<installer-script minSpecVersion="2.0">
    <title>AgentHarbor</title>
    <organization>com.blocksense.agent-harbor</organization>
    <domains enable_anywhere="true"/>
    <options customize="never" require-scripts="false"/>
    <welcome file="Welcome.txt"/>
    <license file="License.txt"/>
    <conclusion file="Conclusion.txt"/>
    <choices-outline>
        <line choice="default">
            <line choice="agent-harbor"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="agent-harbor" visible="false">
        <pkg-ref id="com.blocksense.agent-harbor"/>
    </choice>
    <pkg-ref id="com.blocksense.agent-harbor" version="1.0.0" onConclusion="none">#AgentHarbor.pkg</pkg-ref>
</installer-script>
